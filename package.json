{
  "name": "mat-process-thc",
  "private": true,
  "scripts": {
    "next": "next",
    "dev": "node server/index.js",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production node server/index.js",
    "test": "npm run format:check && npm run lint && npm run lint:codeowners && npm run test:all",
    "test:update": "npm run format && npm run lint:fix && npm run lint:codeowners && npm run test:all:update",
    "test:all": "npm run test:unit && npm run test:feature",
    "test:all:update": "npm run test:unit:update && npm run test:feature:update",
    "test:unit": "jest --config jest.config.unit.js",
    "test:unit:update": "npm run test:unit -- --updateSnapshot",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:feature": "NEXT_DIST_DIR=.next-test PORT=3001 concurrently --names 'server,test' 'npm run build && npm start' 'wait-for-localhost ${PORT} && npm run test:feature:noserver' --kill-others --success first",
    "test:feature:noserver": "jest --config jest.config.feature.js --runInBand",
    "test:feature:update": "NEXT_DIST_DIR=.next-test PORT=3001 concurrently --names 'server,test' 'npm run build && npm start' 'wait-for-localhost ${PORT} && npm run test:feature:noserver -- --updateSnapshot' --kill-others --success first",
    "test:feature:watch": "NEXT_DIST_DIR=.next-test PORT=3001 concurrently --names 'server,test' 'npm start' 'wait-for-localhost ${PORT} && npm run test:feature:noserver -- --watch' --kill-others --success first",
    "lint": "eslint --report-unused-disable-directives --config ./.eslintrc.full.js '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:codeowners": "codeowners-enforcer -i 'package.json' -i 'package-lock.json'",
    "format": "prettier --write '**/*'",
    "format:check": "prettier --check '**/*'"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "cross-fetch": "^3.0.4",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-online": "^8.2.1",
    "jsonwebtoken": "^8.5.1",
    "lbh-frontend-react": "0.0.7",
    "next": "^9.1.8-canary.16",
    "null-as-undefined": "^0.2.1",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.12.0",
    "react-async-hook": "^3.6.1",
    "react-dom": "^16.12.0",
    "remultiform": "0.0.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/jest-axe": "^3.2.1",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-test-renderer": "^16.9.1",
    "@types/selenium-webdriver": "^4.0.6",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "codeowners-enforcer": "^1.0.3",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.0.3",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-circus": "^24.9.0",
    "jest-cucumber": "^2.0.11",
    "jest-date-mock": "^1.0.8",
    "jest-fetch-mock": "^3.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "normalize.css": "^8.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.12.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "styled-jsx": "^3.2.4",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5",
    "wait-for-localhost-cli": "^1.1.0",
    "yn": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
